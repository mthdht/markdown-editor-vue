{
  "name": "markdown-editor-vuejs",
  "version": "1.1.1",
  "description": "A light markdown editor with vuejs",
  "repository": {
    "type": "git",
    "url": "https://github.com/mthdht/markdown-editor-vue.git"
  },
  "main": "dist/markdown-editor.ssr.js",
  "module": "dist/markdown-editor.esm.js",
  "unpkg": "dist/markdown-editor.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "src/**/*.css"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "babel-plugin-prismjs": "^1.0.2",
    "marked": "^0.6.3",
    "material-design-icons": "^3.0.1",
    "prismjs": "^1.16.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "minimist": "^1.2.0",
    "rollup": "^1.12.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.2"
  },
  "keywords": [
    "markdown editor",
    "vue js markdown editor",
    "vue js",
    "markdown"
  ]
}
